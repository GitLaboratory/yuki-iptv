#!/bin/sh
#
# Folder structure (for maintainers):
# astroncia_iptv.py => usr/share/AstronciaIPTV/astroncia_iptv.py
# data => usr/share/AstronciaIPTV/data
#
cd /usr/share/AstronciaIPTV
export LD_LIBRARY_PATH="$(./data/modules/astroncia/pfs.py)"
./astroncia_iptv.py "$@"
exit_code="$?"
reset || true
exit "${exit_code}"
